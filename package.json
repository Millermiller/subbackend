  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb",
      "@vue/typescript"
    ],
    "rules": {
      "no-underscore-dangle": "off",
      "comma-dangle": "off",
      "import/order": "off",
      "object-curly-newline": "off",
      "no-param-reassign": "off",
      "class-methods-use-this": "off",
      "lines-between-class-members": "off",
      "import/no-unresolved": "off",
      "no-shadow": "off",
      "max-len": "off",
      "no-nested-ternary": "off",
      "semi": "off",
      "import/named": "off",
      "import/newline-after-import": "off",
      "import/prefer-default-export": "off",
      "import/no-cycle": "off",
      "camelcase": "off",
      "no-plusplus": "off",
      "no-console": "off",
      "no-restricted-imports": "off",
      "import/first": "off",
      "no-lonely-if": "off"
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
